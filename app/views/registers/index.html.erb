<p id="notice"><%= notice %></p>

<h1>Registers</h1>

<table>
  <thead>
    <tr>
      <th>Weight</th>
      <th>Department</th>
      <th>Laboratory</th>
      <th>Residue</th>
      <th>Collection</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @registers.each do |register| %>
      <tr>
        <td><%= register.weight %></td>
        <td><%= register.department %></td>
        <td><%= register.laboratory %></td>
        <td><%= register.residue %></td>
        <td><%= register.collection %></td>
        <td><%= link_to 'Show', register %></td>
        <td><%= link_to 'Edit', edit_register_path(register) %></td>
        <td><%= link_to 'Destroy', register, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Register', new_register_path %>
